doctype html
html(ng-app="myApp")
  head
    meta(charset='utf8')
    base(href='/')
    title DC Dashboard
    link(rel='stylesheet', href='/css/bootstrap.min.css')
    link(rel='stylesheet', href='/css/app.css')
    link(href="http://netdna.bootstrapcdn.com/font-awesome/4.0.0/css/font-awesome.css" rel="stylesheet")
  body
    .container
        .masthead
        h3.text-muted Health Inspections
        ul.nav.nav-justified
            li
                a(href='/') Home
            li
                a(href='pestwatch') Pest Related
            li
                a(href='about') About
            li
                a(href='contact') Contact
        block content

    script(src='js/lib/underscore-min.js')
    script(src='https://ajax.googleapis.com/ajax/libs/angularjs/1.2.6/angular.min.js')
    script(src='js/lib/ui-bootstrap-tpls-0.9.0.min.js')
    script(src='js/lib/angular-route.min.js')
    script(src='js/app.js')
    script(src='js/controllers.js')
    script(src='js/filters.js')

